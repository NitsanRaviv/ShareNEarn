<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         id="WebApp_ID" version="2.5">

    <servlet>
        <servlet-name>signin</servlet-name>
        <servlet-class>facebook4j.examples.signin.SignInServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signin</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>callback</servlet-name>
        <servlet-class>facebook4j.examples.signin.CallbackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>callback</servlet-name>
        <url-pattern>/callback</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>facebook4j.examples.signin.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>post</servlet-name>
        <servlet-class>facebook4j.examples.signin.PostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>post</servlet-name>
        <url-pattern>/post</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>tokenInfo</servlet-name>
        <servlet-class>facebook4j.examples.signin.TokenInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tokenInfo</servlet-name>
        <url-pattern>/tokenInfo</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>gencode</servlet-name>
        <servlet-class>facebook4j.examples.signin.device.GenDeviceCodeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gencode</servlet-name>
        <url-pattern>/gencode</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>deviceSignin</servlet-name>
        <servlet-class>facebook4j.examples.signin.device.DeviceSigninServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deviceSignin</servlet-name>
        <url-pattern>/deviceSignin</url-pattern>
    </servlet-mapping>

    <!--Elad-->
    <servlet>
        <servlet-name>webhook</servlet-name>
        <servlet-class>facebook4j.examples.signin.webhook</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>webhook</servlet-name>
        <url-pattern>/webhook</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>onSharePostServlet</servlet-name>
        <servlet-class>com.elad.OnSharedPostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>onSharePostServlet</servlet-name>
        <url-pattern>/onSharePost</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Cert</servlet-name>
        <servlet-class>com.elad.Cert</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Cert</servlet-name>
        <url-pattern>/.well-known/acme-challenge/xxxxxxxxxxxx-yyyy.zzzzzzzzzzzzzzzzzzz</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

</web-app>